<cftry>
   <cfparam name="attributes.units">
   <cfparam name="attributes.length">
   <cfcatch>
      You need to supply attributes for "units" (legal values are "characters", "words", or "paragraphs") and "length".
   <cfabort>
   </cfcatch>   
</cftry>   

<cfswitch expression="#attributes.units#">

   <cfcase value="characters">
      <cfset output = left( greeking, attributes.length )>
   </cfcase>

   <cfcase value="words">
      <cfset output = "">
      <cfset ctr = "1">
      <cfloop list="#greeking#" index="aWord" delimiters=" ">
         <cfif ctr LTE attributes.length>
            <cfset output = ListAppend( output, aWord, " " )>
         </cfif>
         <cfset ctr = ctr + 1>
      </cfloop>
   </cfcase>
   
   <cfcase value="sentences">
      <cfset output = "">
      <cfset ctr = "1">
      <cfloop list="#greeking#" index="aSentence" delimiters=".">
         <cfif ctr LTE attributes.length>
            <cfset output = ListAppend( output, aSentence, "." )>
         </cfif>
         <cfset ctr = ctr + 1>
      </cfloop>
   </cfcase>
   
   <cfdefaultcase>
      Sorry, That's not a legal value for "units".
   </cfdefaultcase>

</cfswitch>

<cfoutput>#output#.</cfoutput>

